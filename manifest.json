{
"manifest_version": 3, 
"name": "OctoPartSearch",
"version": "0.1",
"description": "Octopart search for Chrome. Select text and search on octopart.",
 "icons": {
	 
    "16": "icon_16.png",
    "48": "icon_48.png",	
    "128": "icon_128.png",	
    "256": "icon_256.png"
  },
"background": {
    "service_worker": "background.js",
    "type": "module" //optional
  },

"permissions": [
"contextMenus",
"background",
"tabs"
]

}